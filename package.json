{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "awesome-debounce-promise": "^2.1.0",
    "connected-react-router": "^6.7.0",
    "date-fns": "^2.10.0",
    "date-fns-tz": "^1.0.10",
    "expect": "^25.1.0",
    "express": "^4.17.1",
    "file-saver": "^2.0.2",
    "formik": "^2.1.4",
    "fuse.js": "^3.4.6",
    "history": "^4.10.1",
    "lodash.get": "^4.4.2",
    "node-fetch": "^2.6.0",
    "prop-types": "^15.7.2",
    "querystring": "^0.2.0",
    "react": "^16.13.0",
    "react-day-picker": "^7.4.0",
    "react-dom": "^16.13.0",
    "react-redux": "7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-virtualized": "^9.21.2",
    "recharts": "^1.8.5",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "styled-components": "^5.0.1",
    "vanilla-autofill-event": "^1.0.3",
    "yup": "^0.28.1"
  },
  "scripts": {
    "start": "env-cmd -e dev react-scripts start",
    "build-qa": "env-cmd -e qa react-scripts build",
    "build-demo": "env-cmd -e demo react-scripts build",
    "build-prod": "env-cmd -e prod react-scripts build",
    "test": "env-cmd -e test react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow",
    "lint": "eslint src",
    "lint-css": "stylelint './src/**/*.js'",
    "test-ci": "env-cmd -e test yarn lint && CI=true yarn test --maxWorkers=2 --coverage --coverageReporters=cobertura --reporters=default --reporters=jest-junit",
    "test-staged": "env-cmd -e test yarn test --watchAll=false --coverage --coverageReporters=cobertura ---reporters=default --reporters=jest-junit --onlyChanged --passWithNoTests",
    "prettier": "prettier-eslint --write '/src/**/*.{js|jsx}'",
    "prettier-dry-run": "prettier-eslint --write '/src/**/*.{js|jsx}' -l debug"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "yarn lint-css",
      "yarn test-staged",
      "git add"
    ]
  },
  "devDependencies": {
    "@redux-saga/testing-utils": "^1.1.3",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "cross-env": "^7.0.1",
    "env-cmd": "^10.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-redux-saga": "^1.1.3",
    "fetch-mock": "^9.1.0",
    "flow-bin": "^0.119.1",
    "husky": "^4.2.3",
    "jest-junit": "^10.0.0",
    "jest-styled-components": "^7.0.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "prettier-eslint-cli": "^5.0.0",
    "react-test-renderer": "^16.13.0",
    "redux-mock-store": "^1.5.4",
    "stylelint": "^13.2.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "flow && lint-staged",
      "pre-push": "eslint src --max-warnings 0"
    }
  }
}
