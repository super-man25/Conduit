  "scripts": {
    "start": "env-cmd base,dev react-scripts start",
    "build-qa": "env-cmd base,qa react-scripts build",
    "build-demo": "env-cmd base,demo react-scripts build",
    "build-prod": "env-cmd base,prod react-scripts build",
    "test": "env-cmd base,test react-scripts test --env=jsdom",
    "eject": "env-cmd base react-scripts eject",
    "lint": "eslint src",
    "test-ci": "yarn lint && yarn test --coverage --coverageReporters=cobertura --testResultsProcessor jest-junit",
    "test-staged": "yarn test --coverage --coverageReporters=cobertura --testResultsProcessor jest-junit --findRelatedTests",
    "precommit": "flow && lint-staged",
    "prettier": "prettier-eslint --write '/src/**/*.{js|jsx}'",
    "prettier-dry-run": "prettier-eslint --write '/src/**/*.{js|jsx}' -l debug"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "yarn test-staged",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
