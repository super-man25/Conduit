{
  "name": "ed-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "awesome-debounce-promise": "^2.1.0",
    "connected-react-router": "^6.4.0",
    "date-fns": "^1.30.1",
    "date-fns-timezone": "^0.1.4",
    "expect": "^24.8.0",
    "express": "^4.17.1",
    "file-saver": "^2.0.1",
    "fuse.js": "^3.4.5",
    "history": "^4.6.3",
    "lodash.get": "^4.4.2",
    "polished": "^3.4.0",
    "prop-types": "^15.6.1",
    "querystring": "^0.2.0",
    "react": "^16.8.6",
    "react-day-picker": "^7.3.0",
    "react-dom": "^16.8.6",
    "react-loadable": "^5.4.0",
    "react-popper": "^1.3.3",
    "react-redux": "6.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "react-virtualized": "^9.20.0",
    "recharts": "^1.6.2",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.3",
    "reselect": "^4.0.0",
    "styled-components": "^4.3.2",
    "vanilla-autofill-event": "^1.0.3",
    "yup": "^0.27.0"
  },
  "scripts": {
    "start": "env-cmd -e dev react-scripts start",
    "build-qa": "env-cmd -e qa react-scripts build",
    "build-demo": "env-cmd -e demo react-scripts build",
    "build-prod": "env-cmd -e prod react-scripts build",
    "test": "env-cmd -e test react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow",
    "lint": "eslint src",
    "lint-css": "stylelint './src/**/*.js'",
    "test-ci": "env-cmd -e test yarn lint && CI=true yarn test --coverage --coverageReporters=cobertura --reporters=default --reporters=jest-junit",
    "test-staged": "env-cmd -e test yarn test --watchAll=false --coverage --coverageReporters=cobertura ---reporters=default --reporters=jest-junit --onlyChanged --passWithNoTests",
    "prettier": "prettier-eslint --write '/src/**/*.{js|jsx}'",
    "prettier-dry-run": "prettier-eslint --write '/src/**/*.{js|jsx}' -l debug"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "yarn lint-css",
      "yarn test-staged",
      "git add"
    ]
  },
  "devDependencies": {
    "@redux-saga/testing-utils": "^1.0.3",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "cross-env": "^5.1.4",
    "env-cmd": "^9.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "^3.10.3",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.7.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.1",
    "eslint-plugin-redux-saga": "^1.0.0",
    "fetch-mock": "^7.3.3",
    "flow-bin": "^0.102.0",
    "husky": "^2.4.1",
    "jest-junit": "^6.4.0",
    "jest-styled-components": "^6.3.3",
    "lint-staged": "^8.2.1",
    "prettier": "^1.17.1",
    "prettier-eslint-cli": "^4.7.1",
    "react-test-renderer": "^16.8.6",
    "redux-mock-store": "^1.5.1",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "flow && lint-staged"
    }
  }
}
