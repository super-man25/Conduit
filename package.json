    "fetch-mock": "^6.3.0",
    "flow-bin": "^0.75.0",
    "husky": "^0.14.3",
    "jest-junit": "^3.6.0",
    "jest-styled-components": "^5.0.1",
    "lint-staged": "^7.0.5",
    "prettier": "^1.12.1",
    "prettier-eslint-cli": "^4.7.1",
    "react-test-renderer": "^16.3.2",
    "redux-mock-store": "^1.5.1"
  }
