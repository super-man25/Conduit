{
  "name": "ed-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "date-fns": "^1.29.0",
    "expect": "^22.4.3",
    "express": "^4.16.3",
    "history": "^4.6.3",
    "polished": "^1.9.2",
    "prop-types": "^15.6.1",
    "querystring": "^0.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-height": "^3.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.1",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "styled-components": "^3.2.3",
    "vanilla-autofill-event": "^1.0.3"
  },
  "scripts": {
    "start": "env-cmd base,dev react-scripts start",
    "build-qa": "env-cmd base,qa react-scripts build",
    "build-prod": "env-cmd base,prod react-scripts build",
    "test": "env-cmd base,test react-scripts test --env=jsdom",
    "eject": "env-cmd base react-scripts eject",
    "lint": "eslint src",
    "test-ci": "yarn test --coverage --coverageReporters=cobertura --testResultsProcessor jest-junit",
    "test-staged": "cross-env CI=true react-scripts test --env=jsdom --findRelatedTests",
    "precommit": "lint-staged",
    "prettier": "prettier --write src/**/*.{js,json}"
  },
  "lint-staged": {
    "src/**/*.{js,json}": ["eslint --fix", "yarn test-staged", "git add"]
  },
  "devDependencies": {
    "@dialexa/eslint-config-dialexa": "^4.1.1",
    "cross-env": "^5.1.4",
    "env-cmd": "^7.0.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "fetch-mock": "^6.3.0",
    "husky": "^0.14.3",
    "jest-junit": "^3.6.0",
    "jest-styled-components": "^5.0.1",
    "lint-staged": "^7.0.5",
    "prettier": "^1.12.1",
    "react-test-renderer": "^16.3.2"
  }
}
