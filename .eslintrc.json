{
  "parser": "babel-eslint",
  "extends": [
    "react-app",
    "plugin:prettier/recommended",
    "plugin:flowtype/recommended",
    "prettier/react"
  ],
  "plugins": ["jest", "redux-saga", "flowtype", "import"],
  "env": {
    "jest/globals": true
  },
  "rules": {
    "flowtype/generic-spacing": [0, "never"],
    "camelcase": 2
  }
}
